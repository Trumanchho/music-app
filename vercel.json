{
    "version": 2,
    "public": true,
    "name": "music-player-app",
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/music-player-app/**",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/dist/music-player-app/$1"
      }
    ]
  }